<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:Name="MuddyTree">
    
    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1" x:Key="controllerBarGradient">
        <GradientStop Color="#171615" Offset="0"/>
        <GradientStop Color="#171615" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1" x:Key="headerGradient">
        <GradientStop Color="#000000" Offset="0"/>
        <GradientStop Color="#11100f" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1" x:Key="sidebarGradient">
        <GradientStop Color="#000000" Offset="0"/>
        <GradientStop Color="#11100f" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1" x:Key="mainBackground">
        <GradientStop Color="#33312e" Offset="0"/>
        <GradientStop Color="#33312e" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="roundedButtonHighlight" Color="#7F344537"/>
    <SolidColorBrush x:Key="detailButtonColor" Color="#484540"/>
    <SolidColorBrush x:Key="detailButtonHighlight" Color="#68645d"/>
    <SolidColorBrush x:Key="normalButtonColor" Color="#1d221d"/>
    <SolidColorBrush x:Key="textColor" Color="White"/>
    <SolidColorBrush x:Key="controllerOptionMenuBackground" Color="#1d221d"/>
</ResourceDictionary>